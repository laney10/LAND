	return false; 
	}
	
	if(document.getElementById("captchab").value==_langData.RNCAPTCHA || document.getElementById("captchab").value=="")
	{
	alert(_langData.RNPLEASE_CAPTCHA);
	return false; 
	}   
	return true;
}
function submitMsg3(){
	if(document.getElementById("select").value==_langData.RNNAME ||document.getElementById("select").value=="" )
	{
	alert(_langData.RNPLEASE_SELECT_COUNTRY);
	return false; 
	}
	
	return true;
}

function submitMsg4(){
    if(document.getElementById("numbera").value=='Product number' ||document.getElementById("numbera").value=="" )
	{
	alert('Please enter your Product number');
	return false; 
	}
	
	if(document.getElementById("batcha").value=='Batch number' ||document.getElementById("batcha").value=="" )
	{
	alert('Please enter your Batch number');
	return false; 
	}
	
	if(document.getElementById("captchaa").value==_langData.RNCAPTCHA || document.getElementById("captchaa").value=="")
	{
	alert(_langData.RNPLEASE_CAPTCHA);
	return false; 
	}   
	return true;
}

