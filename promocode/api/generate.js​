// api/generate.js
module.exports = (req, res) => {
  // 设置响应头
  res.setHeader('Content-Type', 'application/json');
  res.setHeader('x-content-type-options', 'nosniff');
  
  // 简单响应
  res.status(200).json({
    success: true,
    message: "Generate API is working!",
    timestamp: new Date().toISOString()
  });
};
